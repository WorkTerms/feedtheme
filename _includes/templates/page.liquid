{%- assign theme_exist = false -%}
{%- for static_file in site.static_files -%}
	{%- if static_file.path == '_includes/templates/leads{{ base }}.html' -%}
		{%- assign theme_exist = true -%}
	{%- endif -%}
{%- endfor -%}			
{%- if theme_exist -%}
	{%- include templates/leads{{ base }}.html -%}
{%- else -%}			
	{%- include templates/leads/pages.html -%}
{%- endif -%}
