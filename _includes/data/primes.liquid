{%- assign num 2                                -%}
{%- assign order = 1                            -%}
{%- assign group = 1                            -%}
{%- assign baseurl = "/"                        -%}
{%- assign primes = '' | split: ','             -%}
{%- include core/parts.liquid                   -%}

{%- if site.data.roots                          -%}

{%-   assign my_roots = site.data.roots 
        | where_exp:'item', 'item.key > 10000'  -%}

{%-   for item in my_roots                      -%}
{%-     assign num = item.key                   -%}
{%-     include core/parts.liquid               -%}
{%-   endfor                                    -%}

{%- else                                        -%}
{%-   include data/_primes.liquid               -%}
{%- endif                                       -%}
