{%- assign num = 2                              -%}
{%- assign group = 1                            -%}
{%- assign baseurl = "/"                        -%}
{%- include core/parts.liquid                   -%}

{%- if site.data.roots                          -%}

{%-   assign my_roots = site.data.roots         -%}

{%-   for item in my_roots                      -%}
{%-     assign num = item.key | times: 1        -%}
{%-     if num < 100000                         -%}
{%-       include core/parts.liquid             -%}
{%-     endif                                   -%}
{%-   endfor                                    -%}

{%- else                                        -%}
{%-   include data/_primes.liquid               -%}
{%- endif                                       -%}
