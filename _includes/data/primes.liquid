{%- assign sqrt = 2                       -%}
{%- assign prime = '1,2' | split: ','     -%}
{%- assign n = include.n | default: 100   -%}

{%- assign max = n | times: n             -%}
{%- for num in (3 .. max)                 -%}
{%-   assign mod = num | modulo: 2        -%}
{%-   if mod == 0                         -%}
{%-     continue                          -%}
{%-   endif                               -%}

{%-   for i in (sqrt .. n)                -%}
{%-     assign pow = i | times: i         -%}
{%-     if pow >= num                     -%}
{%-       assign sqrt = i                 -%}
{%-       break                           -%}
{%-     endif                             -%}
{%-   endfor                              -%}

{%-   assign bad = false                  -%}
{%-   for i in (3 .. sqrt)                -%}
{%-     assign mod = num | modulo: i      -%}
{%-     if mod == 0                       -%}
{%-       assign bad = true               -%}
{%-       break                           -%}
{%-     endif                             -%}
{%-   endfor                              -%}

{%-   if bad                              -%}
{%-     continue                          -%}
{%-   endif                               -%}

{%-   assign prime = prime | push: num    -%}
{%- endfor                                -%}
