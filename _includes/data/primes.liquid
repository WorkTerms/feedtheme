{%- assign MAX = 10000                    -%}
{%- assign MAX_SQRT = 100                 -%}

{%- assign id = 1                         -%}
{%- assign sqrt = 2                       -%}
{%- assign prime = '2,3' | split: ','     -%}

{%- for num in (3 .. MAX)                 -%}
{%-   assign mod = num | modulo: 2        -%}
{%-   if mod == 0                         -%}
{%-     continue                          -%}
{%-   endif                               -%}

{%-   assign bad = false                  -%}

{%-   for i in (sqrt .. MAX_SQRT)         -%}
{%-     assign pow = i | times: i         -%}
{%-     if pow >= num                     -%}
{%-       assign sqrt = i                 -%}
{%-       break                           -%}
{%-     endif                             -%}
{%-   endfor                              -%}

{%-   for i in (3 .. sqrt)                -%}
{%-     assign mod = num | modulo: i      -%}
{%-     if mod == 0                       -%}
{%-       assign bad = true               -%}
{%-       break                           -%}
{%-     endif                             -%}
{%-   endfor                              -%}

{%-   if bad                              -%}
{%-     continue                          -%}
{%-   endif                               -%}

{%- assign prime = prime | push: num      -%}
{%- endfor                                -%}
