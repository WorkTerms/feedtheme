{%- assign prime = '1,2' | split: ','     -%}
{%- assign n = include.n | default: 100   -%}
{%- assign min = include.min | default: 2 -%}

{%- assign max = n | times: n             -%}
{%- for num in (3 .. max)                 -%}
{%-   assign mod = num | modulo: 2        -%}
{%-   if mod == 0                         -%}
{%-     continue                          -%}
{%-   endif                               -%}

{%-   for i in (min .. n)                 -%}
{%-     assign pow = i | times: i         -%}
{%-     if pow >= num                     -%}
{%-       assign min = i                  -%}
{%-       break                           -%}
{%-     endif                             -%}
{%-   endfor                              -%}

{%-   assign bad = false                  -%}
{%-   for i in (3 .. min)                 -%}
{%-     assign mod = num | modulo: i      -%}
{%-     if mod == 0                       -%}
{%-       assign bad = true               -%}
{%-       break                           -%}
{%-     endif                             -%}
{%-   endfor                              -%}

{%-   if bad                              -%}
{%-     continue                          -%}
{%-   endif                               -%}

{%-   assign prime = prime | push: num    -%}
{%- endfor                                -%}
