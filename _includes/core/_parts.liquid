{%- if id <= 102                           -%}
{%-   assign lexer = id                    -%}
{%-   assign parser = id                   -%}
{%-   assign syntax = 0                    -%}
{%- elsif id <= 329                        -%}
{%-   assign lexer = 102                   -%}
{%-   assign parser = id                   -%}
{%-   assign syntax = 0                    -%}
{%- else                                   -%}
{%-   assign lexer = 102                   -%}
{%-   assign parser = 329                  -%}
{%-   assign syntax = id | minus: 329      -%}
{%- endif                                  -%}

{%- assign bond  = lexer                   -%}
{%- assign tag = _bond | jsonify           -%}
{%- assign details = '('
       | append: bond
       | append: ','
       | append: lexer
       | append: ','
       | append: parser
       | append: ','
       | append: syntax
       | append: ')'
       | append: tag                 -%}

{%- for prime in primes                    -%}
{%-   assign loop = forloop.index          -%}
{%-   if id <= prime                       -%}
{%-     break                              -%}
{%-   endif                                -%}
{%- endfor                                 -%}

{%- assign stage = '[' | append: num       -%}
{%- for item in array                      -%}
{%-   assign size = item | size            -%}
{%-   assign stage = stage
			| append: ';'
			| append: size                 -%}
{%- endfor                                 -%}
{%- assign stage = stage 
       | append: ']('
       | append: loop                             
	   | append: ')'                       -%}

{%- include core/node.liquid               -%}
