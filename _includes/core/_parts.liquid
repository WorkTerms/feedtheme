{%- assign data_id = '('
      | append: bond
      | append: ';'
      | append: lexer
      | append: ';'
      | append: parser
      | append: ';'
      | append: syntax
      | append: ')'                           -%}

{%- for item in primes                        -%}
{%-   assign loop = forloop.index             -%}
{%-   if id <= item                           -%}
{%-     break                                 -%}
{%-   endif                                   -%}
{%- endfor                                    -%}

{%- assign flowto = '['                       -%}
{%- for item in array reversed                -%}
{%-   assign size = item | size | minus: 1    -%}
{%-   assign lead = primes[size]              -%}
{%-   assign flowto = flowto 
          | append: lead
          | append: ','                       -%}
{%- endfor                                    -%}

{%- assign size = size | plus: 1              -%}
{%- assign aria_flowto = flowto 
      | append: prime
      | append: ']('
      | append: size
      | append: ')'                           -%}
		  
