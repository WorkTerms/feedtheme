{%- assign tag = ""                         -%}
{%- assign baseurl = "/"                    -%}

{%-	assign eq = 0 -%}
{%- assign width = 0 -%}
{%- assign weight = 0 -%}
{%-	assign expanse = 0 -%}

{%- assign id = '' | split: ',' -%}
{%- assign arm = '' | split: ',' -%}
{%- assign node = '' | split: ',' -%}
{%- assign parts = '' | split: ',' -%}

{%- for prime in primes                     -%}
{%-   assign array = '' | split: ','          -%}
{%-   assign order = forloop.index          -%}

{%-   if primes contains order               -%}
{%-     assign parts = parts | push: order   -%}
{%-     assign width = parts | size          -%}

{%-     if parts contains width              -%}
{%-       assign node = node | push: width   -%}
{%-       assign weight = node | size        -%}

{%-       if node contains weight            -%}
{%-         assign arm = arm | push: weight  -%}
{%-	        assign expanse = arm | size      -%}

{%-	        if arm contains expanse          -%}
{%-	          assign id = id | push: expanse -%}
{%-	          assign eq = id | size          -%}

{%-	        endif                            -%}
{%-	      endif                              -%}
{%-     endif                                -%}

{%-     assign array = array | push: parts     -%}
{%-     assign array = array | push: node      -%}
{%-     assign array = array | push: arm       -%}
{%-     assign array = array | push: id        -%}

{%-     include core/node.liquid              -%}

{%-     for item in array                      -%}
{%-       include core/_parts.liquid           -%}
{%-     endfor                                 -%}

{%-   endif                                  -%}
{%- endfor                                 -%}
