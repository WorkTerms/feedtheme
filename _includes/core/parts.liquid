{%- assign tag = "[]"                     -%}

{%- assign primes = primes | push: num    -%}
{%- assign order = primes | size          -%}
{%- assign probe = order                  -%}

{%- if primes contains probe              -%}

{%-   unless array                        -%}
{%-     assign array = '' | split: ','    -%}
{%-   endunless                           -%}

{%-   include primes/bond.liquid          -%}

{%-   if num > 1000 and order < _brake    -%}

{%-     assign id = id | plus: 1          -%}
{%-     assign parser = id                -%}

{%-     assign syntax = id | minus: 329   -%}
{%-     include core/_parts.liquid        -%}

{%-   endif                               -%}

{%- endif                                 -%}

{%- if num < 1000 or order >= _brake      -%}

{%-   assign id = id | plus: 1            -%}
{%-   assign syntax = id | minus: 329     -%}
{%-   assign parser = id                  -%}
{%-   assign lexer = id                   -%}

{%-   if order >= _brake                  -%}
{%-     assign parser = 329               -%}

{%-     assign lexer = id
          | minus: syntax
          | minus: 160		              -%}
		  
{%-   endif                               -%}

{%-   include core/_parts.liquid          -%}

{%- endif                                 -%}
