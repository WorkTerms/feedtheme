{%- assign tag = "[]"                     -%}

{%- assign primes = primes | push: num    -%}
{%- assign order = primes | size          -%}
{%- assign probe = order                  -%}

{%- if primes contains probe              -%}

{%-   include primes/bond.liquid          -%}

{%- endif                                 -%}

{%- if num < 1000 or order >= _brake      -%}

{%-   assign id = id | plus: 1            -%}
{%-   assign syntax = id | minus: 329     -%}
{%-   assign parser = id                  -%}
{%-     assign lexer = id
          | minus: syntax
          | minus: 60
          | minus: 60
          | minus: 40
          | minus: 1		              -%}
		  
{%-     assign bond = lexer
          | minus: 60
          | minus: 6		              -%}
		  
{%-     assign lexer = lexer
          | minus: bond		              -%}
		  

{%-   if order >= _brake                  -%}
{%-     assign parser = 329               -%}

{%-     assign lexer = id
          | minus: syntax
          | minus: 60
          | minus: 60
          | minus: 40
          | minus: 1		              -%}
		  
{%-     assign bond = lexer
          | minus: 60
          | minus: 6		              -%}
		  
{%-     assign lexer = lexer
          | minus: bond		              -%}
		  
{%-   endif                               -%}

{%-   include core/_parts.liquid          -%}

{%- endif                                 -%}
