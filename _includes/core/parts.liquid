{%- assign tag = "[]"                     -%}

{%- assign primes = primes | push: num    -%}
{%- assign probe = primes | size          -%}
{%- assign order = probe                  -%}

{%- if primes contains order              -%}

{%-   unless parts                        -%}
{%-     assign parts = '' | split: ','    -%}
{%-   endunless                           -%}

{%-   include core/_bond.liquid           -%}

{%-   if num > 1000 and probe < _brake    -%}
{%-     include core/_parts.liquid        -%}
{%-   endif                               -%}

{%- endif                                 -%}

{%- if num < 1000 or probe >= _brake      -%}
{%-   include core/_parts.liquid          -%}
{%- endif                                 -%}
