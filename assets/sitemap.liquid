{%- comment -%}
*
*  This loop loops through all collection called `feeds` with category 'section'
*  and sorts it by the front matter variable 'weight' and than collect
*  each of the location into the indexed sitemap
*
*  You may see the running code here:
*  https://eq19.github.io/sitemap.xml
*
{%- endcomment -%}

{%- include extra/rest/defaults.liquid -%}
{%- include leads/rest/defaults.liquid -%}

<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/sitemap.xsl"?>

{%- if site.baseurl == "" and branch == "eQ19"%}
	{%- include tabs.liquid -%}<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	{%- for i in (1..1000) -%}
		{%- include tabs.liquid size=1 -%}<sitemap>
		{%- include tabs.liquid size=2 -%}<loc>https://www.eq19.com/sitemap/index_{{ i }}.xml</loc>
		{%- include tabs.liquid size=2 -%}<lastmod>2017-02-16T04:00:08-05:00</lastmod>
		{%- include tabs.liquid size=1 -%}</sitemap>
	{%- endfor -%}
	{%- include tabs.liquid -%}</sitemapindex>
{%- else -%}
	{%- include sitemap.liquid -%}
{%- endif -%}
